{{#each hits}}
<div class="photo-card">
  <a href="{{largeImageURL}}"></a>
  <img src="{{webformatURL}}" alt="{{tags}}" width="280" height="220" loading="lazy" />
  <div class="info">
    <p class="info-item">
      <b>Likes:{{likes}}</b>
    </p>
    <p class="info-item">
      <b>Views:{{views}}</b>
    </p>
    <p class="info-item">
      <b>Comments:{{comments}}</b>
    </p>
    <p class="info-item">
      <b>Downloads:{{Downloads}}</b>
    </p>
  </div>
</div>
{{/each}}